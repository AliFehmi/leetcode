class Solution { // time limit exceeds!
public:
    bool containsDuplicate(vector<int>& nums) {
        for (unsigned int i=0; i< nums.size()-1; i++){
            for (unsigned int j=i+1; j < nums.size(); j++){
               
                if (nums[i]== nums[j]){
                    return true;
                }
            }
        }
        return false;
    }
};
